[effect_script*]
{
	name	=apprentice_frost;

	script	=[[
//			sound play s_e_attack_frostnid at #SOURCE_POSITION dist 3 20;

			sfx create lightning #TARGET_KB 
				"must_update()scale(1.5)bolt_life(1)mindisplace(0)maxdisplace(0)
				color0(.2,.2,1)[0][1]";
			set $bolt #POP;
			sfx target $bolt source;
			sfx attach_point $bolt @weapon_bone source;
			sfx start $bolt;

			sfx create lightning #TARGET_KB 
				"must_update()scale(1)bolt_life(1)mindisplace(0)maxdisplace(0)
				color0(.5,.5,1)dur(.3)";
			set $bolt2 #POP;
			sfx target $bolt2 source;
			sfx attach_point $bolt2 @weapon_bone source;
			sfx start $bolt2;
			
			sfx create fire #TARGET_KB 
				"must_update()scale(.25)color0(.5,.5,1)line()dur(.55)velocity(0,-15,0)
				instant()accel(0,15,0)";
			sfx target #PEEK source;
			sfx attach_point #PEEK @weapon_bone source;
			sfx start #POP;

			sfx create explosion #TARGET_KB 
				"must_update()vmin(2)vmax(3)omni_dir()ivel(0,0,0)rvel(2,0,2)
				scale_range(0,.3,0)texture(b_sfx_sparkle01)color0(.5,.5,1)
				color1(0,.05,.05)dur(1)radius(.4)fade_range(2,2,0)count(32)";
			sfx freeze_targets #PEEK;
			sfx start #POP;

			sfx create explosion #TARGET_KB 
				"must_update()srate(.01)vmin(2)vmax(3)omni_dir()ivel(0,0,0)
				rvel(2,0,2)scale_range(0,.3,0)
				color0(.5,.5,1)color1(0,.05,.05)dur(.66)radius(.4)
				fade_range(2,2,0)count(32)";
			sfx freeze_targets #PEEK;
			sfx start #POP;
			]];
}

