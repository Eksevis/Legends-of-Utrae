			[chore_fidget]
			{
				chore_stances = 0;
				skrit = select_fidget;
				[anim_files] { 00=dff; }
			}
			[chore_walk]
			{
				chore_stances = 0;
				skrit = select_walk;
				[anim_files] { 00=wl; }
			}
		}
	}

	[inventory]
	{
		//selected_active_location = il_active_primary_spell;
		//[other] { il_active_primary_spell=spell_gorack_deathball; }
	}
	[common] 
	{ 
		screen_name="Gorack Fistgrinder"; 

		membership = party_gorack_boss, monster;
	}
	[defend] { defense=701.6; }
   	[inventory]
	{
   		[pcontent]
		{
			[oneof*]
			{
				[oneof*]
				{
				   chance = 0.05;
				   il_main = potion_health_super;
				   il_main = potion_mana_super;
				}
				[all*]
				{
       				chance = 0.02;
	                il_main = #spell/53-76;
				}
				[oneof*]
				{
					chance = 0.2;
					il_main = #*/251-293;
					il_main = #armor/129-673;
	                il_main = #weapon/251-293;
				}
				[oneof*]
				{
	            	chance = 0.004;
	                il_main = #weapon/-rare(1)/279-363;
	                il_main = #armor/-rare(1)/859-1074;
                    il_main = #*/-rare(1)/279-363;
                }
				[oneof*]
				{
                	chance = 0.002;
	                il_main = #weapon/-unique(2)/279-363;
	                il_main = #armor/-unique(2)/859-1074;
                    il_main = #*/-unique(2)/279-363;
	            }
				[gold*]
				{
					chance = 0.125;
					max = 378743;
					min = 278621;
				}
			}
		}
	}
	[mind]
	{

		jat_fidget		= world\ai\jobs\common\job_fidget.skrit
			?curious		= 0.75
			&interest		= interest
			&meander		= false
			&still			= false;

		jat_brain							= world\ai\jobs\common\brain_hero.skrit
			?actor_joins_existing_party			= true;

		jat_attack_object_melee = world\ai\jobs\common\job_attack_object_melee.skrit
			?run_chance 			= 1.0;

		actor_auto_switches_to_magic = true;
		melee_engage_range = 14.0;
		personal_space_range = 0.7;
		ranged_engage_range = 14.0;
		sight_range = 14.0;

	}

}

