[t:template,n:ut3w_vines_killer]
{
  category_name = "1W_evil_d";
  doc = "dsx_vines_killer";
  specializes = actor_evil;
	  [actor]
	    {
	      [skills]
			{
			strength =  	66, 0;
			intelligence =  7.6, 0;
			dexterity =  	18.2, 0;
            		melee =  	96,  0;
			}
	    }
	  [aspect]
	    {
	    	draw_shadow = false;
		experience_value = 3390000;
		life = 2112;
		max_life = 2112;
		mana = 30.5;
		max_mana = 30.5;
			model = m_c_edm_ug_pos_1;
	    	textures:0 = b_c_edm_vines;
	    	//material = flesh;
	      	[voice]
		    {
		      		[attack]		{ priority = always;	* = s_e_attack_whip;	}
				[die]			{ priority = high;	* = s_e_die_vines;		}
				[enemy_spotted]	{ priority = high; * = s_e_ground_burst_SED;}
			}
	    }
	  [attack]
	    {
	      attack_range = 3.5;
	      damage_max = 452; 
	      damage_min = 435; 
	    }
	
	  [common]
	    {
	      screen_name = "Twisted Redwood Roots"; //--
	      membership = monster; //--

	    }
	  [defend] { defense = 1353; }
	
	[body]
	{
		avg_move_velocity = 0;
		[bone_translator]
		{
			body_anterior = bone20;
			body_mid = bone13;
			body_posterior = bone04;
			kill_bone = bone09;
			shield_bone = bone01;
		}
		[chore_dictionary]
		{
			initial_chore = chore_none;
			chore_prefix = a_c_edm_ug_fs;
			[chore_attack]
			{
				chore_stances = 0;
				skrit = select_attack;
				[anim_files]
				{
					00 = at;
					01 = at-02;
				}
			}
			[chore_default]
			{
				chore_stances = 0;
				skrit = infinite_loop;
				[anim_files] { 00=ds; }
			}
			[chore_misc]
			{
				chore_stances = 0;
				skrit = select_transition;
				[anim_files]
				{
					slep = ds;
					wake = rv;
				}
			}
			[chore_die]
			{
				chore_stances = 0;
				skrit = select_transition;
				[anim_files] { 00=di; }
			}
			[chore_fidget]
			{
				chore_stances = 0;
				skrit = select_fidget;
				[anim_files] { 00=dff; }
			}
			[chore_walk]
			{
				chore_stances = 0;
				skrit = select_walk;
				[anim_files] { 00=dfs; }
			}
		}
	}
	[mind]
	{
		jat_brain				= world\ai\jobs\common\brain_hero.skrit
		?actor_joins_existing_party		= false
		&actor_creates_own_party		= false;
		
		jat_die 			= world\ai\jobs\actors\evil\job_die_delete.skrit;
		jat_startup 			= world\ai\jobs\common\job_startup_reveal.skrit
			?invisible 			= true;
		jat_attack_object_melee = world\ai\jobs\common\job_attack_object_melee.skrit
			?no_turn			= false
			&hit_multiple			= true;	
		jat_flee_from_object		= jat_none;
		jat_guard         			= jat_none;
		jat_move          			= jat_none;
		jat_patrol         			= jat_none;

		actor_auto_fidgets 			= false;
		movement_orders 			= mo_hold;
		personal_space_range 		= 0.7;
		on_enemy_entered_ocz_attack = true;
		sight_origin_height			= 2.5;
		
		actor_weapon_preference		= WP_MELEE;
		actor_auto_switches_to_magic	= false;
		actor_auto_switches_to_karate	= false;
		actor_auto_switches_to_melee	= true;
		
		melee_engage_range 			= 3;
		ranged_engage_range 		= 3.0;
		sight_range 				= 3.0;
		inner_comfort_zone_range 	= 3.0;
		outer_comfort_zone_range 	= 3.0;
		com_range				 	= 3.0;
	}

}
