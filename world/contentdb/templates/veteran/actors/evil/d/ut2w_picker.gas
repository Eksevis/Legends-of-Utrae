[t:template,n:ut2W_picker]
{
	category_name = "2W_evil_d";
	doc = "2W_picker";
	specializes = 2W_base_picker;
	[actor]
	{
		[skills]
		{
			strength =  	25.2, 0;
			intelligence =  9, 0;
			dexterity =  	26.7, 0;
            melee = 		65, 0;
		}
	}
	[aspect]
	{
		experience_value = 137800;
		life = 1705.3;
		max_life = 1705.3;
		mana = 21.3;
		max_mana = 21.3;
		[voice]
		{
			[die]	{ priority = high; * = s_e_die_spider; }
		}
	}
	[attack]
	{
		attack_range = 0.5;
		damage_max = 293.1;
		damage_min = 228.3;
		reload_delay = 0;
	}
	[common] { screen_name="Rabid Picker"; }
	[defend] { defense=742.9; }
	[mind]
	{
		jat_fidget	= world\ai\jobs\common\job_fidget.skrit
			?meander	= false
			&still		= false;
	}
	[physics]
	{
		explode_when_killed = true;
		break_effect = blood_mist;
		break_sound = s_e_meat_explosion;
		gib_gore_good = true;
		[break_particulate]
		{
			frag_glb_picker_01 = 1;
			frag_glb_picker_02 = 1;
			frag_glb_picker_03 = 3;
			frag_glb_picker_04 = 2;
			frag_glb_picker_05 = 3;
		}
	}
}

